<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<display-name>mensadigitale</display-name>
	<servlet>
		<servlet-name>PrenotazioneServlet</servlet-name>
		<display-name>PrenotazioneServlet</display-name>
		<description></description>
		<servlet-class>business.prenotazioni.PrenotazioneServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QRCodeServlet</servlet-name>
		<display-name>QRCodeServlet</display-name>
		<description></description>
		<servlet-class>business.prenotazioni.QRCodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PrenotazioneServlet</servlet-name>
		<url-pattern>/prenotazione</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QRCodeServlet</servlet-name>
		<url-pattern>/qrcode</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>